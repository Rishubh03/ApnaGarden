{
  "expo": {
    "name": "ApnaGarden",
    "slug": "ApnaGarden",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.rishubh.ApnaGarden"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d74c8279-07c8-43eb-9433-617a2d7607e1"
      }
    },
    "plugins": [
      [
      "react-native-vision-camera",
      {
        "cameraPermissionText": "Apna Garden needs access to your Camera.",
        "enableMicrophonePermission": true,
        "microphonePermissionText": "Apna Garden needs access to your Microphone."
      }
    ],
    [
      "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
    ]
    ]
  }
}
